{
    "name": "web-components-repo",
    "private": true,
    "changelog": {
        "labels": {
            "feat": ":rocket: New Feature",
            "bug": ":bug: Bug Fix",
            "doc": ":memo: Documentation",
            "internal": ":house: Internal",
            "breaking": ":boom: Breaking Change"
        }
    },
    "scripts": {
        "build": "lerna exec --ignore=@gaopeng123/utils -- npm run build",
        "test": "lerna exec --ignore=@gaopeng123/utils -- npm run test",
        "push": "lerna publish"
    },
    "devDependencies": {
        "lerna": "^4.0.0"
    },
    "workspaces": [
        "packages/image-upload",
        "packages/message",
        "packages/rc-image-upload",
        "packages/login-module",
        "packages/rc-login-module",
        "packages/multi-player",
        "packages/rc-multi-player"
    ]
}
